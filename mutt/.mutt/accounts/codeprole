# Account Settings -----

set from      = "code.prole@gmail.com"
set sendmail  = "/usr/local/bin/msmtp -a codeprole"

# Default inbox.
set spoolfile = "+codeprole-gmail.com/INBOX"

# Other special folders.
set mbox      = "+codeprole-gmail.com/archive"
set postponed = "+codeprole-gmail.com/drafts"
set record    = "+codeprole-gmail.com/sent"

color status green default
