{
  "name"        : "es5class",
  "main"        : "index.js",
  "version"     : "2.3.1",
  "homepage"    : "https://github.com/pocesar/ES5-Class",
  "authors"     : [
    "Paulo Cesar <email@pocesar.e4ward.com>"
  ],
  "description" : "Prototypal inheritance based on ES5 Object.create and Object.defineProperty for node.js and modern browsers",
  "moduleType"  : [
